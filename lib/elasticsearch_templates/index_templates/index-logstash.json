{
  "index_patterns": [
    "logstash-*"
  ],
  "composed_of": [
    "settings-common",
    "mappings-corecommon",
    "mappings-string",
    "mappings-ip_address",
    "mappings-geohash",
    "mappings-port",
    "mappings-zeekuid",
    "mappings-final"
  ],
  "template": {
    "mappings": {
      "properties": {
        "syslog_pid": {
          "type": "integer"
        },
        "ttl": {
          "type": "integer"
        },
        "key_id": {
          "type": "keyword"
        }
      }
    }
  },
  "_meta": {
    "provider": "SOF-ELK"
  }
}
