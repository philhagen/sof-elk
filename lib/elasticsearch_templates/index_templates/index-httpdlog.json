{
  "index_patterns": [
    "httpdlog-*"
  ],
  "composed_of": [
    "settings-common",
    "mappings-corecommon",
    "mappings-string",
    "mappings-ip_address",
    "mappings-geohash",
    "mappings-port",
    "mappings-zeekuid",
    "mappings-final"
  ],
  "template": {
    "mappings": {
      "properties": {
        "httpversion": {
          "type": "keyword"
        },
        "ident": {
          "type": "keyword"
        },
        "response_code": {
          "type": "short"
        },
        "request_method": {
          "type": "keyword"
        }
      }
    }
  },
  "_meta": {
    "provider": "SOF-ELK"
  }
}
